{
  "name": "subtle-ui",
  "description": "A collection of clever yet understated user interactions found on the web",
  "version": "1.0.3",
  "repository": "https://github.com/ryanwiemer/subtle-ui",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.20.3",
    "@mdx-js/tag": "^0.20.3",
    "babel-plugin-styled-components": "^1.10.0",
    "babel-standalone": "^6.26.0",
    "gatsby": "^2.3.3",
    "gatsby-mdx": "0.4.4",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-nprogress": "^2.0.10",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-filesystem": "^2.0.28",
    "prismjs": "^1.16.0",
    "react": "^16.8.5",
    "react-confetti": "^2.7.0",
    "react-dom": "^16.8.5",
    "react-helmet": "^5.2.0",
    "react-live": "^2.0.0",
    "styled-components": "^4.2.0"
  },
  "keywords": [
    "subtle-ui"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "test": "yarn run lint:css && yarn run lint:js && yarn run check-pretty",
    "clean": "gatsby clean",
    "share": "gatsby develop --host $(ifconfig | awk '/inet 192\\.168\\.[0-9]+\\.[0-9]+/{print $2}') --port 8000",
    "prod": "gatsby build && gatsby serve"
  },
  "devDependencies": {
    "eslint": "^5.15.3",
    "eslint-config-gatsby-standard": "^2.1.1",
    "prettier": "^1.16.4",
    "stylelint": "9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.6.0"
  }
}
