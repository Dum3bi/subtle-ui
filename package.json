{
  "name": "subtle-ui",
  "description": "A collection of clever yet understated user interactions found on the web",
  "version": "0.0.1",
  "repository": "https://github.com/ryanwiemer/subtle-ui",
  "author": "Ryan Wiemer <ryan@ryanwiemer.com>",
  "dependencies": {
    "@mdx-js/mdx": "^0.15.0",
    "@mdx-js/tag": "^0.15.0",
    "babel-plugin-styled-components": "^1.6.3",
    "babel-standalone": "^6.26.0",
    "gatsby": "next",
    "gatsby-image": "next",
    "gatsby-mdx": "0.1.3",
    "gatsby-plugin-google-analytics": "next",
    "gatsby-plugin-netlify": "next",
    "gatsby-plugin-nprogress": "next",
    "gatsby-plugin-react-helmet": "next",
    "gatsby-plugin-sharp": "next",
    "gatsby-plugin-styled-components": "next",
    "gatsby-source-filesystem": "next",
    "react": "^16.4.2",
    "react-confetti": "^2.3.0",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-live": "^1.11.0",
    "styled-components": "^3.4.5"
  },
  "keywords": [
    "subtle-ui"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "serve": "gatsby serve",
    "develop": "gatsby develop",
    "prod": "npm run build && npm run serve",
    "test": "npm run lint:css && npm run lint:js && npm run check-pretty",
    "dev": "gatsby develop",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier -l {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-gatsby-standard": "^2.0.0",
    "prettier": "^1.14.2",
    "stylelint": "9.5.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.3.2"
  },
  "browserslist": [
    "> 1%",
    "IE >= 9",
    "last 2 versions"
  ]
}
